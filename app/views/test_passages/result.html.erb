<h1><%= t('.header', title: @test_passage.test.title) %></h1>
<% if @test_passage.success? %>
  <p><%= t('.success') %></p>
  <p class="positive_result"><%= t('.result', total: @test_passage.result) %></p>
<% else %>
  <p><%= t('.fail') %></p>
  <p class="negative_result"><%= t('.result', total: @test_passage.result) %></p>
<% end %>
<div>
  <%= link_to t('.all'), tests_path %>
</div>
